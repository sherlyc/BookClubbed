{
  "name": "boilerplate-react-webpack",
  "version": "0.1.1",
  "description": "A webpack React starter",
  "repository": "https://github.com/dev-academy-challenges/boilerplate-react-webpack.git",
  "main": "server/index.js",
  "scripts": {
    "dev": "nodemon --watch server server/index & webpack --watch",
    "dev-client": "webpack-dev-server --hot --inline --content-base public",
    "dev-server": "nodemon --watch server server/index",
    "test": "ava -v 'test/**/*.test.js'",
    "dbtest": "ava -v 'test/**/books.test.js'",
    "knex": "knex",
    "start": "node server/index"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "author": "EDA",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.18.0",
    "enzyme": "^2.8.2",
    "jsdom": "^11.0.0",
    "nodemon": "^1.11.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.1.1",
    "react-test-renderer": "^15.5.4",
    "sqlite3": "^3.1.8",
    "supertest": "^3.0.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "cors": "^2.7.1",
    "express": "^4.14.0",
    "goodreads-api-node": "0.0.4",
    "knex": "^0.13.0",
    "superagent": "^2.0.0"
  }
}
